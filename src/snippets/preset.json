{
  "ez-modal-react-antd": {
    "key": "ezModalAntd",
    "prefix": "ezModalAntd",
    "description": "通用的ez-modal-react代码模板（Creates a Standard Ez-Modal-React Component with Antd Modal and TypeScript）",
    "body": [
      "import { useEffect } from 'react'",
      "import EasyModal, { InnerModalProps } from 'ez-modal-react'",
      "import { Modal } from 'antd'",
      "",
      "interface IProps {}",
      "",
      "type IModalProps = InnerModalProps<unknown /* props.hide() args value type*/> & IProps",
      "",
      "export const CommonModal = EasyModal.create((props: IModalProps) => {",
      "  const handleOk = async () => {",
      "    const result = await Promise.resolve('result type' as any)",
      "    if (result === 'result type') props.hide(result)",
      "    else props.hide(null)",
      "  }",
      "",
      "  const handleCancel = () => props.hide(null)",
      "",
      "  useEffect(() => {",
      "    if (!props.visible) return",
      "",
      "    console.log('hello')",
      "  }, [props.visible])",
      "",
      "  return (",
      "    <Modal",
      "      open={props.visible}",
      "      onOk={handleOk}",
      "      onCancel={handleCancel}",
      "      afterClose={props.remove}",
      "    >",
      "      hello",
      "   </Modal>",
      "  )",
      "})",
      "",
      "export default async function openCommonModal(params: IProps) {",
      "  return EasyModal.show(CommonModal, params)",
      "}"
    ],
    "scope": "typescript,typescriptreact,javascript,javascriptreact"
  }
}
